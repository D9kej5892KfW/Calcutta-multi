{
  "dashboard": {
    "id": null,
    "uid": "claude-performance-intel",
    "title": "Claude Performance & Development Intelligence",
    "description": "Performance analytics, workflow intelligence, and resource optimization dashboard for Claude Code agent monitoring",
    "tags": ["claude", "performance", "development", "intelligence", "workflow", "optimization"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-2h",
      "to": "now"
    },
    "style": "dark",
    "theme": "dark",
    "templating": {
      "list": [
        {
          "name": "tool_filter",
          "type": "custom",
          "multi": true,
          "includeAll": true,
          "allValue": ".*",
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "label": "Tool Filter",
          "options": [
            {
              "selected": true,
              "text": "All",
              "value": "$__all"
            },
            {
              "selected": false,
              "text": "Read",
              "value": "Read"
            },
            {
              "selected": false,
              "text": "Write", 
              "value": "Write"
            },
            {
              "selected": false,
              "text": "Edit",
              "value": "Edit"
            },
            {
              "selected": false,
              "text": "Bash",
              "value": "Bash"
            },
            {
              "selected": false,
              "text": "Grep",
              "value": "Grep"
            },
            {
              "selected": false,
              "text": "TodoWrite",
              "value": "TodoWrite"
            },
            {
              "selected": false,
              "text": "Glob",
              "value": "Glob"
            },
            {
              "selected": false,
              "text": "MultiEdit",
              "value": "MultiEdit"
            },
            {
              "selected": false,
              "text": "Task",
              "value": "Task"
            },
            {
              "selected": false,
              "text": "WebFetch",
              "value": "WebFetch"
            }
          ],
          "query": "Read,Write,Edit,Bash,Grep,TodoWrite,Glob,MultiEdit,Task,WebFetch",
          "refresh": 0,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "⚡ Avg Response Time",
        "type": "stat",
        "description": "Average tool execution response time - Target: <200ms",
        "targets": [
          {
            "expr": "avg(avg_over_time({service=\"claude-telemetry\"} |~ \"duration.*[0-9]+\" | regexp \"duration.*?([0-9]+)\" | unwrap duration [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#10B981", "value": null},
                {"color": "#3B82F6", "value": 100},
                {"color": "#F59E0B", "value": 300},
                {"color": "#EF4444", "value": 500}
              ]
            },
            "unit": "ms",
            "min": 0,
            "decimals": 0
          }
        },
        "gridPos": {"h": 5, "w": 6, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "title": "📊 Operations/Min",
        "type": "stat",
        "description": "Current operation throughput rate",
        "targets": [
          {
            "expr": "sum(rate({service=\"claude-telemetry\"}[1m])) * 60",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#6B7280", "value": null},
                {"color": "#10B981", "value": 1},
                {"color": "#3B82F6", "value": 5},
                {"color": "#8B5CF6", "value": 10}
              ]
            },
            "unit": "ops/min",
            "min": 0,
            "decimals": 1
          }
        },
        "gridPos": {"h": 5, "w": 6, "x": 6, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "⚠️ Error Rate",
        "type": "stat",
        "description": "Percentage of failed operations - Target: <5%",
        "targets": [
          {
            "expr": "(sum(rate({service=\"claude-telemetry\"} |~ \"(?i)error|fail|exception\"[5m])) / sum(rate({service=\"claude-telemetry\"}[5m]))) * 100",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#10B981", "value": null},
                {"color": "#F59E0B", "value": 2},
                {"color": "#EF4444", "value": 5}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "decimals": 1
          }
        },
        "gridPos": {"h": 5, "w": 6, "x": 12, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 4,
        "title": "👥 Active Sessions",
        "type": "stat",
        "description": "Number of currently active Claude sessions",
        "targets": [
          {
            "expr": "count(count by (session) ({service=\"claude-telemetry\"} | json | __error__ != \"JSONParserErr\" | session != \"\" [5m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "#6B7280", "value": null},
                {"color": "#3B82F6", "value": 1},
                {"color": "#8B5CF6", "value": 3},
                {"color": "#F59E0B", "value": 5}
              ]
            },
            "unit": "short",
            "min": 0,
            "decimals": 0
          }
        },
        "gridPos": {"h": 5, "w": 6, "x": 18, "y": 0},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 5,
        "title": "📈 Tool Performance Trends",
        "type": "timeseries",
        "description": "Response time trends by tool type with performance thresholds",
        "targets": [
          {
            "expr": "avg by (tool) (avg_over_time({service=\"claude-telemetry\"} |~ \"tool_name.*($tool_filter)\" |~ \"duration.*[0-9]+\" | json | __error__ != \"JSONParserErr\" | unwrap duration [1m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{tool}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 4,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 500}
              ]
            },
            "unit": "ms",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "right", "showLegend": true}
        }
      },
      {
        "id": 6,
        "title": "🚀 Throughput Analysis",
        "type": "timeseries",
        "description": "Operations per minute by tool type showing activity patterns",
        "targets": [
          {
            "expr": "sum by (tool) (rate({service=\"claude-telemetry\"} |~ \"tool_name.*($tool_filter)\" | json | __error__ != \"JSONParserErr\" [1m])) * 60",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{tool}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 30,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 4,
              "stacking": {"mode": "normal", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "unit": "ops/min",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "right", "showLegend": true}
        }
      },
      {
        "id": 7,
        "title": "🎯 Task Completion Analytics",
        "type": "piechart",
        "description": "TodoWrite task status distribution and completion rates",
        "targets": [
          {
            "expr": "sum by (status) (count_over_time({service=\"claude-telemetry\"} |~ \"TodoWrite\" |~ \"status.*(?:pending|in_progress|completed)\" | json | __error__ != \"JSONParserErr\" [2h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "mappings": [
              {"options": {"pending": {"color": "#F59E0B", "index": 0}}, "type": "value"},
              {"options": {"in_progress": {"color": "#3B82F6", "index": 1}}, "type": "value"},
              {"options": {"completed": {"color": "#10B981", "index": 2}}, "type": "value"}
            ],
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 13},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "pieType": "pie",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "right", "showLegend": true, "calcs": ["percent"]},
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 8,
        "title": "🔧 Tool Usage Distribution",
        "type": "barchart",
        "description": "Most frequently used tools in the current time window",
        "targets": [
          {
            "expr": "topk(10, sum by (tool) (count_over_time({service=\"claude-telemetry\"} | json | __error__ != \"JSONParserErr\" | tool != \"\" [2h])))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{tool}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "axisColorMode": "text",
              "axisGridShow": true,
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "orientation": "horizontal"
