          {
            "expr": "sum by (operation) (rate({service=\"claude-telemetry\"} |~ \"(?:Read|Write|Edit|MultiEdit)\" | json | __error__ != \"JSONParserErr\" [1m])) * 60",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{operation}}"
