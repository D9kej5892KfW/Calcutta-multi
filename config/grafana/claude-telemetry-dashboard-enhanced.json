{
  "dashboard": {
    "id": null,
    "uid": "claude-telemetry-enhanced",
    "title": "Claude Agent Telemetry - Security Monitoring Dashboard",
    "description": "Enhanced security-focused dashboard for Claude Code agent monitoring with improved UX and accessibility",
    "tags": ["claude", "telemetry", "security", "monitoring", "enhanced-ui"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-2h",
      "to": "now"
    },
    "style": "dark",
    "theme": "dark",
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "üõ°Ô∏è Security Status",
        "type": "stat",
        "description": "Critical security boundary violations - Should remain at 0",
        "targets": [
          {
            "expr": "sum(count_over_time({service=\"claude-telemetry\"} |= \"outside_project_scope.*true\"[1h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "SECURE"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "#10B981",
                  "value": null
                },
                {
                  "color": "#F59E0B", 
                  "value": 1
                },
                {
                  "color": "#EF4444",
                  "value": 3
                }
              ]
            },
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {
          "h": 5,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 2,
        "title": "‚ö° Activity Rate",
        "type": "stat", 
        "description": "Real-time Claude tool execution rate",
        "targets": [
          {
            "expr": "sum(rate({service=\"claude-telemetry\"}[1m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "#2E86AB",
                  "value": null
                },
                {
                  "color": "#FF6B35",
                  "value": 5
                },
                {
                  "color": "#EF4444",
                  "value": 15
                }
              ]
            },
            "unit": "reqps",
            "decimals": 2
          }
        },
        "gridPos": {
          "h": 5,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "üë• Active Sessions",
        "type": "stat",
        "description": "Currently active Claude Code sessions",
        "targets": [
          {
            "expr": "count(count by (session) (rate({service=\"claude-telemetry\"}[5m])))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "#6366F1",
                  "value": null
                },
                {
                  "color": "#F59E0B",
                  "value": 3
                },
                {
                  "color": "#EF4444",
                  "value": 6
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 5,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 4,
        "title": "üìä Total Operations",
        "type": "stat",
        "description": "Cumulative tool executions in current time window",
        "targets": [
          {
            "expr": "sum(count_over_time({service=\"claude-telemetry\"}[2h]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "#10B981",
                  "value": null
                },
                {
                  "color": "#2E86AB",
                  "value": 100
                },
                {
                  "color": "#FF6B35",
                  "value": 500
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 5,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 5,
        "title": "üîß Tool Usage Trends",
        "type": "timeseries",
        "description": "Real-time usage patterns of Claude Code tools",
        "targets": [
          {
            "expr": "sum by (tool) (rate({service=\"claude-telemetry\"} | json | tool != \"\"[2m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{tool}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 4,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Read"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "#2E86AB",
                    "mode": "fixed"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName", 
                "options": "Write"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "#FF6B35",
                    "mode": "fixed"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Bash"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "#F59E0B",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 5
        },
        "options": {
          "legend": {
            "calcs": ["lastNotNull", "max"],
            "displayMode": "table",
            "placement": "right"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 6,
        "title": "üîç Security Events Analysis",
        "type": "timeseries",
        "description": "Security-focused event categorization and monitoring",
        "targets": [
          {
            "expr": "sum by (event) (rate({service=\"claude-telemetry\"} | json | event != \"\"[2m]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            },
            "legendFormat": "{{event}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 30,
              "gradientMode": "opacity",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "normal"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*security.*|.*violation.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "#EF4444",
                    "mode": "fixed"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*file.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "fixedColor": "#10B981",
                    "mode": "fixed"
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 5
        },
        "options": {
          "legend": {
            "calcs": ["lastNotNull", "mean"],
            "displayMode": "table",
            "placement": "right"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 7,
        "title": "üìÅ File Operations Timeline",
        "type": "logs",
        "description": "Real-time stream of file system operations with security context",
        "targets": [
          {
            "expr": "{service=\"claude-telemetry\"} | json | event =~ \"file.*\" | line_format \"{{.timestamp}} [{{.tool}}] {{.event}} - {{.file_path}}\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "aetvioshimfwge"
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ]
  },
  "overwrite": false
}